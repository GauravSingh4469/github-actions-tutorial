{
    "family": "github-actions-tutorial-task-def-family",
    "networkMode": "awsvpc",
    "containerDefinitions": [{
        "name": "repository1",
        "image": "533267452917.dkr.ecr.us-east-1.amazonaws.com/repository1:latest",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80
        }],
        "essential": true, 
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/github-actions-tutorial-log-group",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::533267452917:role/ecsTaskExecutionRoleSaadGithub",
    "taskRoleArn": "arn:aws:iam::533267452917:role/ecsTaskRole"
}